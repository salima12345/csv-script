# WordPress API settings
WP_API_URL=https://your-wordpress-site.com/wp-json
WP_API_USERNAME=your-admin-username
WP_API_PASSWORD=your-admin-password

# API Endpoints (change if your custom namespace is different)
ARCHITECTS_ENDPOINT=morocco-architecture/v1/architects
BUILDINGS_ENDPOINT=morocco-architecture/v1/buildings

# CSV Files (default paths)
ARCHITECTS_CSV=./data/architects.csv
BUILDINGS_CSV=./data/buildings.csv

# Import settings
BATCH_SIZE=10
DELAY_BETWEEN_REQUESTS=1000